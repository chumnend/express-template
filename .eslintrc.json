{
  "env": {
      "commonjs": true,
      "es2020": true,
      "node": true
  },
  "extends": [
    "eslint:recommended", 
    "prettier"
  ],
  "parserOptions": {
      "ecmaVersion": 11
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["warn"],
    "no-unused-vars": ["error", {"argsIgnorePattern": "next"}]
  }
}